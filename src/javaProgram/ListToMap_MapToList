package com.java;

import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.stream.Collectors;

public class ListToMap_MapToList {

	public static void main(String[] args) {
		// List to Map
		List<Integer> list = Arrays.asList(1, 2, 3, 4, 5, 6, 7, 8, 9);
		Map<Integer, Integer> map = list.stream().collect(Collectors.toMap(Integer::intValue, Integer::intValue));
		map.forEach((k, v) -> {
			//System.out.println(k + " " + v);
		});
		
		// Map TO List
		List<Integer> keyList=map.keySet().stream().collect(Collectors.toList());
		System.out.println("KeyLisy:"+keyList);
		List<Integer> valueList=map.values().stream().collect(Collectors.toList());
		System.out.println("valueList:"+valueList);

	}

}
