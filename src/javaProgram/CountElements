package com.java;

import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class CountElements {

	public static void main(String[] args) {
		List<String> list = Arrays.asList("apple", "banana", "apple", "orange", "banana", "apple");
		Map<String, Long>map=list.stream().collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
		//map.forEach((key, value) -> System.out.println(key + ": " + value));
		
		map.entrySet().stream().filter(entry->entry.getValue()>1).forEach(System.out::println);
		
		
		//remove Duplicate
		map.values().removeIf(x -> x > 1);
		//map.keySet().forEach(System.out::println);
		map.forEach((k,v)->System.out.println(k+" "+v));
		
		String str="aabbccaabccd0011";
		Map<Character, Long>map2=str.chars().mapToObj(c->(char)c).collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
		map2.entrySet().stream().filter(entry->entry.getValue()>1).forEach(System.out::println);
		
		

		
		
	}

}
