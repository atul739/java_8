package com.java;

import java.math.BigInteger;

public class LargeNumberAddition {

	public static void main(String[] args) {
		String number1 = "123456789012345678901234567890";
		String number2 = "987654321098765432109876543210";

		BigInteger bigInteger = new BigInteger(number1);
		BigInteger bigInteger2 = new BigInteger(number2);

		BigInteger sum = bigInteger.add(bigInteger2);
		System.out.println("sum:"+sum);

	}

}
